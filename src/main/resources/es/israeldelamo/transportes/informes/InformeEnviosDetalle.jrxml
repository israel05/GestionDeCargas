    <query language="SQL"><![CDATA[SELECT transportes.`ENVIO`.cod_tracking,
	transportes.`ENVIO`.cod_conductor,
	transportes.`CONDUCTOR`.dni,
	transportes.`CONDUCTOR`.nombre,
	transportes.`CARGA_Y_ENTREGA`.cod_carga,
	transportes.`CARGA_Y_ENTREGA`.dimension_l,
	transportes.`CARGA_Y_ENTREGA`.dimension_h,
	transportes.`CARGA_Y_ENTREGA`.dimension_a,
	transportes.`CARGA_Y_ENTREGA`.peso_unitario,
	transportes.`CARGA_Y_ENTREGA`.pais_origen,
	transportes.`CARGA_Y_ENTREGA`.pais_destino,
	transportes.`ENVIO`.cod_carga_entrega,
	transportes.`CARGA_Y_ENTREGA`.precio,
	transportes.`CARGA_Y_ENTREGA`.cantidad_bultos,
	transportes.`CONDUCTOR`.apellido
FROM transportes.`ENVIO`
	INNER JOIN transportes.`CONDUCTOR` ON 
	 transportes.`ENVIO`.cod_conductor = transportes.`CONDUCTOR`.dni 
	INNER JOIN transportes.`CARGA_Y_ENTREGA` ON 
	 transportes.`ENVIO`.cod_carga_entrega = transportes.`CARGA_Y_ENTREGA`.cod_carga]]></query>
